@{
    ViewData["Title"] = "Home Page";
}
<link href="@Url.Content("~/css/home/index.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/js/home/index.js")" type="text/javascript"></script>
<div id="app">
    <div class="flex">
        <input v-model="message" type="text" class="form-control">
        <h1>{{ message }}</h1>
    </div>
    <div class="flex">
        <input v-model="calculate_value_1" type="text" class="form-control">
        <h1>{{ calculate_value_1 }}</h1>
    </div>
    <div class="flex">
        <input v-model="calculate_value_2" type="text" class="form-control">
        <h1>{{ calculate_value_2 }}</h1>
    </div>
    <div class="flex">
        <h1>{{ calculate_result }}</h1>
    </div>
    <button name="calculate" v-on:click="calculate_click">ボタン</button>
</div>
